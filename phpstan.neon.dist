includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:

    tmpDir: .phpstan.cache

    parallel:
        jobSize: 20
        maximumNumberOfProcesses: 10
        minimumNumberOfJobsPerProcess: 2

    paths:
        - src/

    # Level 9 is the highest level
    level: 9

    ignoreErrors:

    excludePaths:
